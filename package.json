{
    "name": "serve-router",
    "version": "1.2.0",
    "type": "module",
    "sideEffect": false,
    "main": "dist/index.cjs",
    "types": "dist/index.d.ts",
    "files": [
        "src/",
        "dist/"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        },
        "./node": {
            "import": "./dist/node/index.mjs",
            "require": "./dist/node/index.cjs"
        },
        "./utils": {
            "import": "./dist/utils/index.mjs",
            "require": "./dist/utils/index.cjs"
        }
    },
    "scripts": {
        "build": "unbuild",
        "test": "node --experimental-transform-types --test test/*",
        "cov": "node --experimental-transform-types --experimental-test-coverage --test test/*"
    },
    "devDependencies": {
        "@types/node": "^22.17.1",
        "typescript": "^5.9.2",
        "unbuild": "^3.6.0"
    },
    "optionalDependencies": {
        "urlpattern-polyfill": "^10.1.0"
    },
    "license": "MIT",
    "author": "YieldRay",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/YieldRay/serve-router.git"
    },
    "bugs": {
        "url": "https://github.com/YieldRay/serve-router/issues"
    },
    "homepage": "https://github.com/YieldRay/serve-router#readme",
    "publishConfig": {
        "registry": "https://registry.npmjs.org"
    },
    "description": "tiny router library that routes for your web standard http server",
    "keywords": [
        "http",
        "router",
        "workers",
        "worker",
        "serverless"
    ],
    "packageManager": "pnpm@10.14.0"
}
